# .env

# Credentials
SBT_USER=your-username
SBT_PASS=your-password-or-token
SBT_REALM=Artifactory Realm

# Registry host/domain (no path)
SBT_REGISTRY_HOST=example.jfrog.io

# Base URL with /artifactory
SBT_BASE_URL=https://example.jfrog.io/artifactory

# Repositories
SBT_RELEASES=https://example.jfrog.io/artifactory/libs-release
SBT_SNAPSHOTS=https://example.jfrog.io/artifactory/libs-snapshot
SBT_PLUGIN_RELEASES=https://example.jfrog.io/artifactory/sbt-plugins

# Renovate Credentials
RENOVATE_MAVEN_REGISTRY=https://example.jfrog.io/artifactory/libs-release
RENOVATE_IVY_REGISTRY=https://example.jfrog.io/artifactory/sbt-plugins
RENOVATE_HOST=example.jfrog.io        # bare domain, no scheme
RENOVATE_USER=your-username
RENOVATE_PASS=your-password
